  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "scripts": {
    "eslint": "eslint components core routes test tools",
    "stylelint": "stylelint \"src/components/**/*.css\" \"src/routes/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "clean": "node tools/clean",
    "build": "node tools/build --release",
    "build:debug": "node tools/build",
    "start": "webpack-dev-server --progress --hot --inline --content-base src/static"
  },
  "betterScripts": {
  "build:start": {
    "command": "webpack-dev-server --progress --hot --inline --content-base src/static ",
    "env": {
      "NODE_ENV": "development"
    }
  }
},
  "devDependencies": {
    "babel-plugin-import": "^1.1.1",
    "bell-on-bundler-error-plugin": "^2.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
